{
    "name": "infomaniak/laravel-kdrive",
    "description": "Infomaniak kDrive Storage driver for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Hedger",
            "email": "nicolas.hedger@infomaniak.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Infomaniak\\KDrive\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "infomaniak/flysystem-kdrive": "^1.0 || dev-master"
    },
    "require-dev": {
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/vincent-tarrit/flysystem-kdrive.git"
        }
    ],
    "scripts": {
        "check-style": "phpcs src",
        "fix-style": "phpcbf src"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Infomaniak\\KDrive\\KDriveServiceProvider"
            ]
        }
    }
}
