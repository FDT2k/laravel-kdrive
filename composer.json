{
    "name": "infomaniak/laravel-kdrive",
    "description": "Infomaniak kDrive Storage driver for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Hedger",
            "email": "nicolas.hedger@infomaniak.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Infomaniak\\KDrive\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "infomaniak/flysystem-kdrive": "dev-master",
        "illuminate/support": "~9.0|~10.0",
        "league/flysystem": "^3.0",
        "league/flysystem-webdav": "^3.15"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5"
    },
    "scripts": {
        "check-style": "phpcs src",
        "fix-style": "phpcbf src"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Infomaniak\\KDrive\\KDriveServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/cotiga/flysystem-kdrive.git"
        }
    ]
}
